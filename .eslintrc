{
    "rules": {
        "indent": [
            2,
            2
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "no-trailing-spaces": 1,
        "semi": [
            2,
            "always"
        ],
        "quotes": [
            2,
            "single"
        ]
    },
    "env": {
        "node": true,
        "mocha": true
    },
    "extends": "eslint:recommended"
}